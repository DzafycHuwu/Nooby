#include <iostream>

using namespace std;

int main()
{

int niz[100], n;
cout << "Enter amount of numbers that you want:" << endl;
cin >> n;
for (int i=0; i<n; i++) {
cout << "Enter " << i+1 << "." << " number:" << endl;
cin >> niz[i];
}

cout << "These are the numbers that you chose: " << endl;
for(int i=0;i<n;i++) {
cout << niz[i] << endl;
}

return 0;
}
